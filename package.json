{
  "name": "eslint-plugin-importer",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@10.29.2",
  "description": "An ESLint plugin that is a subset of eslint-plugin-import.",
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/sxzz",
  "homepage": "https://github.com/sxzz/eslint-plugin-importer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/eslint-plugin-importer.git"
  },
  "bugs": {
    "url": "https://github.com/sxzz/eslint-plugin-importer/issues"
  },
  "keywords": [
    "eslint",
    "plugin",
    "import"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "eslint-typegen.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsx scripts/typegen.ts && tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.6.1",
    "@sxzz/prettier-config": "^2.3.1",
    "@types/node": "^25.2.2",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript-eslint/utils": "^8.54.0",
    "@typescript/native-preview": "7.0.0-dev.20260209.1",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "eslint-typegen": "^2.3.0",
    "eslint-vitest-rule-tester": "^3.1.0",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "prettier": "@sxzz/prettier-config"
}
